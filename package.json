{
  "name": "basic-web-mud",
  "version": "1.0.0",
  "description": "Typescript implementation of SimpleMUD",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "bs": "webpack --mode development --config wp.server.dev.js",
    "bc": "webpack --mode development --config wp.client.dev.js",
    "build": "npm run bs & npm run bc",
    "dc": "webpack --mode production --config wp.client.prod.js",
    "ds": "webpack --mode production --config wp.server.prod.js",
    "dist": "npm run ds & npm run dc",
    "reload": "webpack-dev-server --mode development --config wp.client.dev.js",
    "start": "cd ./dist/server && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fragarco/BasicWebMUD.git"
  },
  "keywords": [
    "MUD",
    "SimpleMUD",
    "typescript"
  ],
  "author": "Javier Garc√≠a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fragarco/BasicWebMUD/issues"
  },
  "homepage": "https://github.com/fragarco/BasicWebMUD#readme",
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react-test-renderer": "^16.9.3",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "jest": "^26.4.2",
    "sass-loader": "^10.0.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.3",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@hapi/hapi": "^20.0.0",
    "@hapi/inert": "^6.0.2",
    "@types/hapi__hapi": "^19.0.4",
    "@types/hapi__inert": "^5.2.0",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
